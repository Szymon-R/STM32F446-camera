  MX_GPIO_Init();
  MX_DMA_Init();
  MX_DCMI_Init();
  MX_TIM3_Init();
  MX_USART2_UART_Init();


	HAL_DCMI_Start_DMA(&hdcmi,(uint32_t)DCMI_MODE_SNAPSHOT,(uint32_t)data_buffer,size/4);
  while (1)
  {


    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}


void HAL_DCMI_FrameEventCallback	(	DCMI_HandleTypeDef * 	hdcmi	)	
{
	for(int i=0;i<size;++i)
	{
			HAL_UART_Transmit(&huart2,(uint8_t*)&data_buffer[i],size,1000);
		if(size%640==0)
			HAL_UART_Transmit(&huart2,(uint8_t*)'\n',1,1000);
			
	}

}


